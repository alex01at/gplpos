<output>
    <display>
        <line>
            #if ($ticketline.isProductCom()) 
                <text align="left" length="15">*${ticketline.printName()}</text>
            #else
                 <text align="left" length="15">${ticketline.printName()}</text>
            #end
            <text align="right" length="4">x${ticketline.printMultiply()}</text>            
        </line>
        <line>
            <text align="left" length="10">${ticketline.printPrice()}</text>
            <text align="right" length="10">${ticketline.printSubValue()}</text>
        </line>
    </display>
</output>